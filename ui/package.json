{
  "name": "ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "test:ci": "ng test --watch=false --code-coverage --browsers=Chrome_headless",
    "lint": "ng lint --format=stylish",
    "e2e-protractor": "ng e2e",
    "e2e-protractor:ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "e2e-protractor:ci-mobile": "ng e2e --protractor-config=e2e/protractor-ci-mobile.conf.js",
    "e2e-cypress": "cypress run --browser chrome",
    "e2e-cypress:ci": "cypress run --headless --browser chrome",
    "e2e-cypress:ci-mobile": "cypress run --headless --browser chrome --config viewportWidth=411,viewportHeight=731",
    "cypress:open": "cypress open",
    "cypress:open-mobile": "cypress open --config viewportWidth=411,viewportHeight=731",
    "sonarqube": "sonar-scanner"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.3",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "~9.0.3",
    "@angular/compiler": "~9.0.3",
    "@angular/core": "~9.0.3",
    "@angular/forms": "~9.0.3",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "~9.0.3",
    "@angular/platform-browser-dynamic": "~9.0.3",
    "@angular/router": "~9.0.3",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.0.3",
    "@angular/language-service": "~9.0.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.2.1",
    "cypress": "^4.11.0",
    "jasmine-core": "~3.5.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "protractor": "~7.0.0",
    "sonar-scanner": "^3.1.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  }
}
